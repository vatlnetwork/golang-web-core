{
  "sessionRepository": {
    "type": "MongoSessionRepository",
    "config": {
      "mongoConfig": {
        "hostname": "localhost:27017",
        "database": "golangWebCore",
        "authSource": "admin",
        "timeoutSeconds": 10
      },
      "collection": "sessions"
    }
  },
  "userRepository": {
    "type": "MongoUserRepository",
    "config": {
      "mongoConfig": {
        "hostname": "localhost:27017",
        "database": "golangWebCore",
        "authSource": "admin",
        "timeoutSeconds": 10
      },
      "collection": "users"
    }
  }
}
